{
	"description": "pyIPAM - IPAM driver in Python",
	"documentation": "https://github.com/jacekkow/docker-plugin-pyipam",
	"workdir": "/usr/src/app",
	"entrypoint": ["uwsgi", "--ini", "uwsgi.ini"],
	"env": [
		{
			"name": "HOME",
			"value": "/usr/src/app"
		}
	],
	"interface" : {
		"types": ["docker.ipamdriver/1.0"],
		"socket": "pyipam.sock"
	}
}
